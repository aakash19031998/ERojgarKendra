                android:layout_height="wrap_content"
                android:textColor="@color/colorPrimary"
                android:layout_marginLeft="@dimen/_8sdp"
                android:layout_marginTop="@dimen/_16sdp"
                android:textStyle="bold"
                android:text="@string/header_personal_details"></TextView>

            <com.google.android.material.textfield.TextInputLayout
                android:id="@+id/mobile_edit_text_inputlayout"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                app:boxStrokeColor="@color/reg_tc_bg"
                android:layout_marginLeft="@dimen/_8sdp"
                android:layout_marginRight="@dimen/_8sdp"
                android:layout_toRightOf="@+id/header1"
                android:layout_marginTop="@dimen/_8sdp"
                app:hintTextColor="@android:color/black"
                android:layout_toLeftOf="@+id/proceddbuttonlayout"
                style="@style/Widget.MaterialComponents.TextInputLayout.OutlinedBox">

                <com.google.android.material.textfield.TextInputEditText
                    android:id="@+id/name_edit_text"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:maxLines="1"
                    android:singleLine="true"
                    android:drawablePadding="@dimen/_8sdp"
                    android:drawableLeft="@drawable/bg_reg_name"
                    android:hint="@string/name_hint" />

            </com.google.android.material.textfield.TextInputLayout>

            <com.google.android.material.textfield.TextInputLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                app:boxStrokeColor="@color/reg_tc_bg"
                android:layout_marginLeft="@dimen/_8sdp"
                android:layout_marginRight="@dimen/_8sdp"
                android:layout_toRightOf="@+id/header1"
                app:hintTextColor="@android:color/black"
                android:layout_marginTop="@dimen/_5sdp"
                android:layout_toLeftOf="@+id/proceddbuttonlayout"
                style="@style/Widget.MaterialComponents.TextInputLayout.OutlinedBox">

                <com.google.android.material.textfield.TextInputEditText
                    android:id="@+id/phone_number_edit_text"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:inputType="number"
                    android:maxLength="10"
                    android:singleLine="true"
                    android:drawablePadding="@dimen/_8sdp"
                    android:drawableLeft="@drawable/bg_reg_mobile_app_icon"
                    android:maxLines="1"
                    android:hint="@string/phone_no_hint" />

            </com.google.android.material.textfield.TextInputLayout>


            <com.google.android.material.textfield.TextInputLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                app:boxStrokeColor="@color/reg_tc_bg"
                android:layout_marginLeft="@dimen/_8sdp"
                android:layout_marginRight="@dimen/_8sdp"
                android:layout_toRightOf="@+id/header1"
                app:hintTextColor="@android:color/black"
                android:layout_marginTop="@dimen/_5sdp"
                android:layout_toLeftOf="@+id/proceddbuttonlayout"
                style="@style/Widget.MaterialComponents.TextInputLayout.OutlinedBox">

                <com.google.android.material.textfield.TextInputEditText
                    android:id="@+id/email_edit_text"
