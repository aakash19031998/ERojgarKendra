<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    android:id="@+id/mainLnr"
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".registrationscreen.controler.RegisterUserDataActivity">


    <RelativeLayout
        android:id="@+id/status_bar_user_profile"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="@color/white">

        <TextView
            android:id="@+id/maintext"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/place_text"
            android:layout_toRightOf="@+id/backbutton"
            android:textColor="@color/app_colour_name"
            android:textSize="@dimen/_12sdp"
            android:layout_centerVertical="true"
            android:layout_marginTop="@dimen/_14sdp"
            android:layout_marginLeft="@dimen/_10sdp"
            android:textStyle="bold">

        </TextView>

        <TextView

            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_toRightOf="@+id/maintext"
            android:text="@string/me_text"
            android:textColor="@color/black"
            android:textSize="@dimen/_12sdp"
            android:layout_centerVertical="true"

            android:layout_marginTop="@dimen/_14sdp"

            android:textStyle="bold">

        </TextView>

        <ImageView
            android:id="@+id/backbutton"
            android:layout_width="@dimen/_18sdp"
            android:layout_height="@dimen/_18sdp"
            android:text="Back"
            android:textSize="36sp"
            android:src="@drawable/backbutton"
            android:layout_marginLeft="@dimen/_8sdp"
            android:layout_marginTop="@dimen/_8sdp"
            android:visibility="visible"
            android:textStyle="bold" />

        <ImageView
            android:id="@+id/edit"
            android:layout_width="@dimen/_18sdp"
            android:layout_height="@dimen/_18sdp"
            android:text="Back"
            android:textSize="36sp"
            android:layout_alignParentRight="true"
            android:src="@drawable/icon_edit"
            android:layout_marginRight="@dimen/_8sdp"
            android:layout_marginTop="@dimen/_8sdp"
            android:visibility="visible"
            android:textStyle="bold" />
        <ImageView
            android:id="@+id/cancel"
            android:layout_width="@dimen/_18sdp"
            android:layout_height="@dimen/_18sdp"
            android:text="Back"
            android:layout_alignParentRight="true"
            android:src="@drawable/bg_prof_cross"
            android:layout_marginRight="@dimen/_8sdp"
            android:layout_marginTop="@dimen/_8sdp"
            android:visibility="gone" />

    </RelativeLayout>



    <ScrollView
        android:id="@+id/scroll"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_below="@+id/status_bar_user_profile"
        >

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_marginBottom="@dimen/_8sdp"
            android:layout_marginLeft="@dimen/_3sdp"
            android:layout_marginRight="@dimen/_3sdp"
            android:orientation="vertical">




            <include
                android:id="@+id/profile_Header"
