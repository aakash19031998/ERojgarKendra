                android:layout_marginTop="@dimen/_8sdp"
                android:textStyle="bold"
                android:visibility="gone"
                android:text="@string/header_resume"></TextView>


            <RelativeLayout
                android:id="@+id/upload_cv_lnr"
                android:layout_width="match_parent"
                android:layout_height="@dimen/_74sdp"
                android:visibility="gone"
                app:boxStrokeColor="@color/reg_tc_bg"
                android:layout_marginLeft="@dimen/_8sdp"
                android:layout_marginRight="@dimen/_8sdp"
                android:layout_toRightOf="@+id/header1"
                app:hintTextColor="@android:color/black"
                android:layout_marginTop="@dimen/_8sdp"
                android:gravity="center"
                android:background="@drawable/gender_background_drawable"
                android:layout_toLeftOf="@+id/proceddbuttonlayout">

                <ImageView
                    android:id="@+id/upload_cv"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:src="@drawable/bg_reg_upload"
                    android:gravity="center"

                    android:foregroundGravity="center_horizontal"
                    />



                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal"
                    android:gravity="center"
                    android:layout_marginTop="@dimen/_10sdp">

                <TextView
                    android:id="@+id/upload_cv_txt"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textSize="@dimen/_15sdp"
                    android:text="@string/upload_cv"
                    android:singleLine="true"
                    android:gravity="center"
                    android:padding="@dimen/_4sdp"
                    android:layout_marginTop="@dimen/_8sdp"
                    android:layout_below="@+id/upload_cv"
                    android:textColor="@color/colorPrimary"
                    />

                <ImageView
                    android:id="@+id/download_CV"
                    android:layout_width="@dimen/_18sdp"
                    android:layout_height="@dimen/_18sdp"
                    android:src="@drawable/bg_reg_download"
                    android:layout_marginTop="@dimen/_8sdp"
                    android:layout_toRightOf="@id/upload_cv_txt"
                    android:layout_below="@+id/upload_cv"
                    android:foregroundGravity="center_horizontal"
                    />


                </LinearLayout>



            </RelativeLayout>

            <TextView
                android:id="@+id/savebutton"
                android:layout_width="match_parent"
                android:layout_height="@dimen/_34sdp"
                android:text="@string/save"
                android:visibility="gone"
                android:layout_marginTop="@dimen/_16sdp"
                android:textColor="@android:color/white"
                android:layout_marginLeft="@dimen/_30sdp"
                android:layout_marginRight="@dimen/_30sdp"
                android:gravity="center"
                android:background="@drawable/verify_enable"
                android:padding="@dimen/_8sdp"
                android:layout_alignParentBottom="true"
                android:layout_marginBottom="@dimen/_16sdp"
                android:layout_below="@+id/resendotpbutton"></TextView>



        </LinearLayout>

    </ScrollView>
</RelativeLayout>