                        android:button="@null"
                        style="@style/radionbutton_male"
                        />

                    <RadioButton
                        android:id="@+id/female"
                        android:layout_width="@dimen/_40sdp"
                        android:layout_height="@dimen/_40sdp"
                        android:layout_marginLeft="@dimen/_30sdp"
                        android:textStyle="bold"
                        android:tag="Female"
                        style="@style/radionbutton_female"
                        android:button="@null"
                        />



                </RadioGroup>


            </LinearLayout>



